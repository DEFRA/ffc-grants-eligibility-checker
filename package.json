{
  "name": "ffc-grants-eligibility-checker",
  "description": "FFC Grant Eligibility Checker",
  "version": "1.0.3",
  "license": "OGL-UK-3.0",
  "contributors": [
    "Andrew Folga <andrew.folga@equalexperts.com>",
    "Andrew Deacon <andrew.deacon@equalexperts.com>",
    "David Barker <david.barker@equalexperts.com>",
    "Stephen Powell <stephen.powell@esynergy.co.uk>"
  ],
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "eslint \"./**/*.js\" --cache --cache-strategy content --fix",
    "format": "prettier --write \"**/*.js\"",
    "build": "sass -I node_modules src/main.scss public/stylesheets/main.css",
    "start": "nodemon ./src/index.js",
    "prepare": "husky install",
    "snyk-test": "snyk test",
    "snyk-monitor": "snyk monitor"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-syntax-import-assertions": "^7.25.6",
    "@babel/preset-env": "^7.25.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.2.3",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.3",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "sass": "^1.77.6",
    "snyk": "^1.1293.1",
    "standard": "^17.1.2"
  },
  "dependencies": {
    "@hapi/hapi": "^21.3.10",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "dotenv": "^16.4.5",
    "govuk-frontend": "^5.5.0",
    "nunjucks": "^3.2.4",
    "uuid": "^10.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
