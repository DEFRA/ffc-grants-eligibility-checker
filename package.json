{
  "name": "ffc-grants-eligibility-checker",
  "description": "FFC Grant Eligibility Checker",
  "version": "1.0.49",
  "license": "OGL-UK-3.0",
  "contributors": [
    "Andrew Folga <andrew.folga@equalexperts.com>",
    "Andrew Deacon <andrew.deacon@equalexperts.com>",
    "David Barker <david.barker@equalexperts.com>",
    "Stephen Powell <stephen.powell@esynergy.co.uk>"
  ],
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "PORT=0 node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:q": "PORT=0 node --experimental-vm-modules node_modules/jest/bin/jest.js --silent",
    "test:cov": "PORT=0 node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --coverageReporters='text' --coverageReporters='text-summary'",
    "test:cov:q": "PORT=0 node --experimental-vm-modules node_modules/jest/bin/jest.js --silent --coverage --coverageReporters='text' --coverageReporters='text-summary'",
    "test:update:snapshots": "PORT=0 npm test -- -u",
    "lint": "eslint \"./**/*.js\" --cache --cache-strategy content --fix",
    "format": "prettier --write \"**/*.js\"",
    "format:njk": "prettier --write \"**/*.njk\" --ignore-path .prettierignore",
    "build": "node build.js",
    "install": "npm --ignore-scripts i",
    "start": "npm run build && nodemon ./src/index.js",
    "start:debug": "node --inspect-brk=0.0.0.0 ./src/index.js",
    "start:docker": "docker-compose -f docker-compose.yaml -f docker-compose.override.yaml up --build",
    "prepare": "husky install",
    "snyk-test": "snyk test",
    "snyk-monitor": "snyk monitor"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-assertions": "^7.25.6",
    "@babel/preset-env": "^7.25.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "sass": "^1.80.5",
    "snyk": "^1.1293.1",
    "standard": "^17.1.2",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox-memory": "^6.0.2",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/hapi": "^21.3.12",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/yar": "^11.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "govuk-frontend": "^5.5.0",
    "joi": "^17.13.3",
    "nunjucks": "^3.2.4",
    "prettier-plugin-jinja-template": "^2.0.0",
    "uuid": "^10.0.0",
    "xstate": "^4.38.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
